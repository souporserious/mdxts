{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "target": "esnext",
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment"
      }
    }
  },
  "module": {
    "type": "commonjs",
    "ignoreDynamic": true
  }
}

{
  "name": "mdxts",
  "version": "1.7.0",
  "description": "The Content & Documentation SDK for React",
  "author": "Travis Arnold",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/mdxts",
    "directory": "packages/mdxts"
  },
  "bugs": "https://github.com/souporserious/mdxts/issues",
  "homepage": "https://mdxts.dev",
  "keywords": [
    "markdown",
    "md",
    "mdx",
    "typescript",
    "ts",
    "tsx",
    "documentation",
    "docgen",
    "react",
    "design",
    "systems"
  ],
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "mdxts": "./dist/cjs/src/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./assets": {
      "types": "./dist/types/assets/index.d.ts",
      "import": "./dist/esm/assets/index.js",
      "require": "./dist/cjs/src/assets/index.js"
    },
    "./collections": {
      "types": "./dist/types/collections/index.d.ts",
      "import": "./dist/esm/collections/index.js",
      "require": "./dist/cjs/src/collections/index.js"
    },
    "./components": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/esm/components/index.js",
      "require": "./dist/cjs/src/components/index.js"
    },
    "./components/*": {
      "types": "./dist/types/components/*.d.ts",
      "import": "./dist/esm/components/*.js",
      "require": "./dist/cjs/src/components/*.js"
    },
    "./core": {
      "types": "./dist/types/core.d.ts",
      "import": "./dist/esm/core.js",
      "require": "./dist/cjs/src/core.js"
    },
    "./loader": {
      "types": "./dist/types/loader/index.d.ts",
      "import": "./dist/esm/loader/index.js",
      "require": "./dist/cjs/src/loader/index.js"
    },
    "./mdx-plugins": {
      "types": "./dist/types/mdx-plugins/index.d.ts",
      "import": "./dist/esm/mdx-plugins/index.js",
      "require": "./dist/cjs/src/mdx-plugins/index.js"
    },
    "./next": {
      "types": "./dist/types/next/index.d.ts",
      "import": "./dist/cjs/src/next/index.js",
      "require": "./dist/cjs/src/next/index.js"
    },
    "./project": {
      "types": "./dist/types/project/index.d.ts",
      "import": "./dist/esm/project/index.js",
      "require": "./dist/cjs/src/project/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "assets": [
        "./dist/types/assets/index.d.ts"
      ],
      "collections": [
        "./dist/types/collections/index.d.ts"
      ],
      "components": [
        "./dist/types/components/index.d.ts"
      ],
      "components/*": [
        "./dist/types/components/*.d.ts"
      ],
      "core": [
        "./dist/types/core.d.ts"
      ],
      "loader": [
        "./dist/types/loader/index.d.ts"
      ],
      "mdx-plugins": [
        "./dist/types/mdx-plugins/index.d.ts"
      ],
      "next": [
        "./dist/types/next/index.d.ts"
      ],
      "project": [
        "./dist/types/project/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc --outDir dist/esm --declaration true --declarationDir dist/types",
    "build:cjs": "swc src --out-dir dist/cjs --ignore **/*.test.ts --ignore **/*.examples.tsx --config-file .swcrc-cjs",
    "dev": "concurrently \"pnpm build:esm --watch\" \"pnpm build:cjs --watch\"",
    "test": "jest"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.14",
    "@types/glob-parent": "5.1.3",
    "@types/mdast": "^4.0.4",
    "@types/mdx": "^2.0.13",
    "@types/hast": "^3.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/title": "^3.4.3",
    "@types/unist": "^3.0.3",
    "@types/ws": "^8.5.12",
    "concurrently": "^8.2.2",
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype": "^13.0.1",
    "typescript": "^5.5.4",
    "vfile": "^6.0.2",
    "webpack": "^5.93.0"
  },
  "dependencies": {
    "@manypkg/find-root": "^2.2.3",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@next/mdx": "14.2.5",
    "@remark-embedder/core": "^3.0.3",
    "@remark-embedder/transformer-codesandbox": "^3.0.0",
    "@tsxmod/utils": "^0.5.2",
    "case-anything": "^3.1.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "estree-util-value-to-estree": "^3.1.2",
    "fast-glob": "^3.3.2",
    "feed": "^4.2.2",
    "glob-parent": "^6.0.2",
    "gray-matter": "^4.0.3",
    "hast-util-to-string": "^3.0.0",
    "mdast-util-to-string": "^4.0.0",
    "read-pkg-up": "^7.0.1",
    "rehype-infer-reading-time-meta": "^2.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^3.0.1",
    "remark-github": "^12.0.0",
    "remark-smartypants": "^2.1.0",
    "remark-squeeze-paragraphs": "^6.0.0",
    "remark-strip-badges": "^7.0.0",
    "remark-unwrap-images": "^4.0.0",
    "restyle": "^2.0.4",
    "server-only": "0.0.1",
    "shiki": "^1.14.1",
    "title": "^3.5.3",
    "ts-morph": "^23.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "ws": "^8.18.0"
  }
}

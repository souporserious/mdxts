    "@mdx-js/esbuild": "^2.3.0",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "2.3.0",
    "@swc/core": "^1.3.37",
    "@types/github-slugger": "^2.0.0",
    "@types/hast": "^2.3.4",
    "case-anything": "^2.1.10",

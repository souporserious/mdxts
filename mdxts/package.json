{
  "name": "mdxts",
  "version": "0.1.2",
  "description": "Markdown by TypeScript",
  "files": [
    "dist/*"
  ],
  "main": "./dist/index.js",
  "exports": {
    "./bundle": "./dist/bundle/index.js",
    "./next": "./dist/next/index.js",
    "./utils": "./dist/utils/index.js",
    "./watcher": "./dist/watcher/index.js"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "bundle": [
        "./dist/bundle/index.d.ts"
      ],
      "next": [
        "./dist/next/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "watcher": [
        "./dist/watcher/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup index.ts utils/index.ts rehype/index.ts bundle/index.ts watcher/index.ts next/index.ts --dts --format esm,cjs",
    "dev": "pnpm build --watch",
    "test": "jest"
  },
  "peerDependencies": {
    "@mdx-js/react": "^2.0.0",
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "ts-morph": ">=17.0.0"
  },
  "devDependencies": {
    "@mdx-js/react": "*",
    "next": "*",
    "react": "*",
    "react-dom": "*",
    "ts-morph": "*"
  },
  "dependencies": {
    "@mdx-js/esbuild": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@swc/core": "^1.3.35",
    "@types/github-slugger": "^2.0.0",
    "@types/hast": "^2.3.4",
    "case-anything": "^2.1.10",
    "chokidar": "^3.5.3",
    "dprint-node": "^1.0.7",
    "esbuild": "^0.17.10",
    "esbuild-plugin-wasm": "^1.0.0",
    "find-up": "^6.3.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-has-property": "^2.0.1",
    "hast-util-heading-rank": "^2.1.1",
    "hast-util-to-string": "^2.0.0",
    "remark-frontmatter": "^4.0.1",
    "shiki": "^0.14.1",
    "title": "^3.5.3",
    "tree-visit": "^0.1.3",
    "ts-morph": "^17.0.1",
    "type-fest": "^3.6.0",
    "unified": "^10.1.2",
    "unist-builder": "^3.0.1",
    "unist-util-visit": "^4.1.2",
    "unist-util-visit-parents": "^5.1.3",
    "url-slug": "^3.0.4",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "yaml": "^2.2.1"
  }
}
